{
  "name": "awalkindublin",
  "custom": {
    "client": {
      "bucketName": "awidclient"
    },
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": [
        "Content-Type",
        "X-Amz-Date",
        "Authorization",
        "X-Api-Key"
      ]
    }
  },
  "plugins": [
    "serverless-cors-plugin",
    "serverless-client-s3",
    "serverless-offline",
    "./plugins/setupdb.js"
  ]
}
